# Production Secrets Template
# Copy this file to .secrets/production.env and fill in your actual values
# This file should never be committed to version control

# Cloudflare Tunnel Configuration
TUNNEL_TOKEN=your_cloudflare_tunnel_token_here

# Domain Configuration
DOMAIN=your_domain_here

# Traefik Dashboard Authentication
TRAEFIK_DASHBOARD_USERS=user:password_hash

# Jetson Nano Deployment Configuration
JETSON_HOST=192.168.x.x
JETSON_USER=your_user
JETSON_SUDO_PASSWORD=your_sudo_password
JETSON_APP_DIRECTORY=/opt/your_app_path
JETSON_DEPLOYMENT_GROUP=your_group

