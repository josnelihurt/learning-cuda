    {
      "title": "Recent Traces with Logs",
      "type": "table",
      "gridPos": {
        "h": 8,
        "w": 24,
        "x": 0,
        "y": 36
      },
      "id": 7,
      "targets": [
        {
          "expr": "{job=\"host-logs\"} | trace_id != \"\"",
          "refId": "A",
          "datasource": {
            "type": "loki",
            "uid": "P8E80F9AEF21F6940"
          }
        }
      ],
      "transformations": [
        {
          "id": "labelsToFields",
          "options": {
            "mode": "columns",
            "keepLabels": ["trace_id", "level", "caller", "message"]
          }
        }
      ],
      "options": {
        "showHeader": true,
        "footer": {
          "show": false
        }
      },
      "fieldConfig": {
        "defaults": {
          "custom": {
            "align": "auto",
            "filterable": true
          }
        },
        "overrides": [
          {
            "matcher": {
              "id": "byName",
              "options": "trace_id"
            },
            "properties": [
              {
                "id": "links",
                "value": [
                  {
                    "title": "View in Jaeger",
                    "url": "http://localhost:16686/trace/${__value.raw}"
                  }
                ]
              }
            ]
          }
        ]
      }
    },
    {
      "title": "Frontend Layer (OpenTelemetry/WebSocket)",
      "type": "logs",
      "gridPos": {
