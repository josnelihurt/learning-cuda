      },
      "id": 8,
      "targets": [
        {
          "expr": "{job=\"host-logs\"} | json | message=~\"Received OTLP logs|Forwarding browser traces|Traces forwarded successfully\" | line_format \"[{{.level}}] {{.message}}{{if .bytes}} | bytes={{.bytes}}{{end}}{{if .status_code}} | status={{.status_code}}{{end}}\"",
