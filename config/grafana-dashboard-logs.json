{
  "title": "CUDA Learning - Multi-Layer Logs",
  "tags": ["logs", "cuda", "observability"],
  "timezone": "browser",
  "schemaVersion": 38,
  "version": 1,
  "refresh": "5s",
  "uid": "cuda-multi-layer-logs",
  "panels": [
      {
        "title": "C++ Layer (CUDA/spdlog)",
        "type": "logs",
        "gridPos": {
          "h": 8,
          "w": 24,
          "x": 0,
          "y": 0
        },
        "id": 1,
        "targets": [
          {
            "expr": "{container=\"cuda-image-processor\"} |~ \"\\\\[.*\\\\] \\\\[\" | line_format \"{{.log}}\"",
            "refId": "A",
            "datasource": {
              "type": "loki",
              "uid": "P8E80F9AEF21F6940"
            }
          }
        ],
        "options": {
          "showTime": true,
          "showLabels": false,
          "showCommonLabels": false,
          "wrapLogMessage": false,
          "prettifyLogMessage": false,
          "enableLogDetails": true,
          "dedupStrategy": "none",
          "sortOrder": "Descending"
        }
      },
      {
        "title": "Go Layer (Webserver/zerolog) - All Levels",
        "type": "logs",
        "gridPos": {
          "h": 10,
          "w": 24,
          "x": 0,
          "y": 8
        },
        "id": 2,
        "targets": [
          {
            "expr": "{container=\"cuda-image-processor\"} | json | level != \"\" | line_format \"[{{.level}}] {{.caller}} - {{.message}}{{if .error}} | error={{.error}}{{end}}{{if .trace_id}} | trace={{.trace_id}}{{end}}\"",
            "refId": "A",
            "datasource": {
              "type": "loki",
              "uid": "P8E80F9AEF21F6940"
            }
          }
        ],
        "options": {
          "showTime": true,
          "showLabels": false,
          "showCommonLabels": true,
          "wrapLogMessage": true,
          "prettifyLogMessage": false,
          "enableLogDetails": true,
          "dedupStrategy": "none",
          "sortOrder": "Descending"
        }
      },
      {
        "title": "Go Layer - Errors & Warnings Only",
        "type": "logs",
        "gridPos": {
          "h": 6,
          "w": 12,
          "x": 0,
          "y": 18
        },
        "id": 3,
        "targets": [
          {
            "expr": "{container=\"cuda-image-processor\"} | json | level=~\"error|warn\" | line_format \"[{{.level | ToUpper}}] {{.message}}{{if .error}} - {{.error}}{{end}}{{if .trace_id}} [trace: {{.trace_id}}]{{end}}\"",
            "refId": "A",
            "datasource": {
              "type": "loki",
              "uid": "P8E80F9AEF21F6940"
            }
          }
        ],
        "options": {
          "showTime": true,
          "showLabels": false,
          "showCommonLabels": false,
          "wrapLogMessage": true,
          "prettifyLogMessage": false,
          "enableLogDetails": true,
          "dedupStrategy": "none",
          "sortOrder": "Descending"
        },
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            }
          }
        }
      },
      {
        "title": "Go Layer - Info Logs",
        "type": "logs",
        "gridPos": {
          "h": 6,
          "w": 12,
          "x": 12,
          "y": 18
        },
        "id": 4,
        "targets": [
          {
            "expr": "{container=\"cuda-image-processor\"} | json | level=\"info\" | line_format \"{{.message}}{{if .trace_id}} [trace: {{.trace_id}}]{{end}}\"",
            "refId": "A",
            "datasource": {
              "type": "loki",
              "uid": "P8E80F9AEF21F6940"
            }
          }
        ],
        "options": {
          "showTime": true,
          "showLabels": false,
          "showCommonLabels": false,
          "wrapLogMessage": false,
          "prettifyLogMessage": false,
          "enableLogDetails": true,
          "dedupStrategy": "none",
          "sortOrder": "Descending"
        }
      },
      {
        "title": "Log Volume by Level",
        "type": "bargauge",
        "gridPos": {
          "h": 6,
          "w": 8,
          "x": 0,
          "y": 24
        },
        "id": 5,
        "targets": [
          {
            "expr": "sum by (level) (count_over_time({container=\"cuda-image-processor\"} | json | level != \"\" [$__auto]))",
            "refId": "A",
            "datasource": {
              "type": "loki",
              "uid": "P8E80F9AEF21F6940"
            }
          }
        ],
        "options": {
          "orientation": "horizontal",
          "displayMode": "gradient",
          "showUnfilled": true
        },
        "fieldConfig": {
          "defaults": {
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": 0, "color": "blue"},
                {"value": 10, "color": "yellow"},
                {"value": 50, "color": "red"}
              ]
            },
            "unit": "short"
          }
        }
      },
      {
        "title": "Logs Rate Over Time",
        "type": "timeseries",
        "gridPos": {
          "h": 6,
          "w": 16,
          "x": 8,
          "y": 24
        },
        "id": 6,
        "targets": [
          {
            "expr": "sum by (level) (rate({container=\"cuda-image-processor\"} | json | level != \"\" [$__auto]))",
            "refId": "A",
            "datasource": {
              "type": "loki",
              "uid": "P8E80F9AEF21F6940"
            },
            "legendFormat": "{{level}}"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            },
            "custom": {
              "drawStyle": "line",
              "lineInterpolation": "smooth",
              "fillOpacity": 10
            },
            "unit": "reqps"
          }
        },
        "options": {
          "tooltip": {
            "mode": "multi",
            "sort": "desc"
          },
          "legend": {
            "displayMode": "list",
            "placement": "bottom",
            "showLegend": true
          }
        }
      },
      {
        "title": "Recent Traces with Logs",
        "type": "table",
        "gridPos": {
          "h": 8,
          "w": 24,
          "x": 0,
          "y": 30
        },
        "id": 7,
        "targets": [
          {
            "expr": "{container=\"cuda-image-processor\"} | json | trace_id != \"\"",
            "refId": "A",
            "datasource": {
              "type": "loki",
              "uid": "P8E80F9AEF21F6940"
            }
          }
        ],
        "transformations": [
          {
            "id": "labelsToFields",
            "options": {
              "mode": "columns",
              "keepLabels": ["trace_id", "level", "caller", "message"]
            }
          }
        ],
        "options": {
          "showHeader": true,
          "footer": {
            "show": false
          }
        },
        "fieldConfig": {
          "defaults": {
            "custom": {
              "align": "auto",
              "filterable": true
            }
          },
          "overrides": [
            {
              "matcher": {
                "id": "byName",
                "options": "trace_id"
              },
              "properties": [
                {
                  "id": "links",
                  "value": [
                    {
                      "title": "View in Jaeger",
                      "url": "http://localhost:16686/trace/${__value.raw}"
                    }
                  ]
                }
              ]
            }
          ]
        }
      }
  ],
  "time": {
    "from": "now-6h",
    "to": "now"
  },
  "timepicker": {
    "refresh_intervals": ["5s", "10s", "30s", "1m", "5m"]
  }
}

